      },
      "header": {
        "type": "object",
        "properties": {
          "signature": {
            "type": "string"
          },
          "post_id": {
            "type": "integer"
          },
          "update_id": {
            "type": "integer"
          },
          "address": {
            "type": "string"
          },
          "output_hash": {
            "type": "string"
          }
